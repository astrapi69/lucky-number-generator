<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CustomGenerationResultActivity">

    <TextView
        android:id="@+id/lbl_custom_generated"
        android:layout_width="@dimen/layout_eurojackpot_full_width_size"
        android:layout_height="@dimen/layout_height_size"
        android:layout_marginStart="@dimen/left_margin_size"
        android:layout_marginTop="@dimen/top_margin_size"
        android:text="@string/custom_generated_lucky_numbers"
        android:textAlignment="center"
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_custom_generated"
        android:layout_width="@dimen/layout_eurojackpot_full_width_size"
        android:layout_height="214dp"
        android:layout_marginStart="@dimen/left_margin_size"
        android:layout_marginTop="@dimen/top_margin_size"
        android:ems="10"
        android:focusable="false"
        android:inputType="none"
        android:hint="@string/hint_text_custom_generated"
        android:autofillHints=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_custom_generated" />
</androidx.constraintlayout.widget.ConstraintLayout>